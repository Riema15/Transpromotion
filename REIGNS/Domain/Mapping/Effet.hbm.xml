<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">

  <class name="Effet" table="effet">
    <id name="Id" column="ef_id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Nom" column="ef_nom" not-null="true"/>
    <property name="Actif" column="ef_actif" not-null="true"/>
    <property name="EffetScolaire" column="ef_sco"/>
    <property name="EffetSous" column="ef_sous"/>
    <property name="EffetSocial" column="ef_soc"/>
    <property name="EffetSante" column="ef_sante"/>

  </class>

</hibernate-mapping>