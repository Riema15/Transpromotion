<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">

  <class name="Carte" table="carte">
    <id name="Id" column="carte_id" type="int">
      <generator class="native"></generator>
    </id>

    <property name="NumEvent" column="carte_num_event" not-null="true"/>
    <property name="Text" column="carte_txt" not-null="true"/>
    <property name="ObjetPossible" column="carte_obj" not-null="true"/>


    <many-to-one name="Rep1" unique="true" cascade="all" column="rep_id1" />
    <many-to-one name="Rep2" unique="true" cascade="all" column="rep_id2" />
    <many-to-one name="Personnage" unique="true" cascade="all" column="pers_id" />
    
  </class>

</hibernate-mapping>