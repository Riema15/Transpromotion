<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">

  <class name="Personnage" table="personnage">
    <id name="Id" column="pers_id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="NomPerso" column="pers_nom" not-null="true"/>
    <property name="Image" column="pers_image" not-null="true"/>
    <property name="Couleur" column="pers_couleur" />
    <property name="Annee" column="pers_annee" not-null="true"/>
    <property name="Bureau" column="pers_bureau" />
    <property name="EtreParrain" column="pers_parrain" />
    

  </class>

</hibernate-mapping>