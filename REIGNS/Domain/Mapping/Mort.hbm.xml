<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">

  <class name="Mort" table="mort">
    <id name="Id" column="mort_id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Nom" column="mort_nom" not-null="true"/>
    <property name="Image" column="mort_image" not-null="true"/>
  
  </class>

</hibernate-mapping>